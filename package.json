{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "dev": "yarn lint && ts-node-dev -r tsconfig-paths/register src/index.ts",
    "clean": "rimraf ./dist/*",
    "tsc": "tsc --build tsconfig.json",
    "build": "yarn lint && yarn clean && yarn tsc && yarn copy",
    "lint": "eslint \"src/**/*.{js,ts}\" --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate": "yarn -r tsconfig-paths/register typeorm migration:run",
    "migrate:generate": "yarn typeorm migration:generate -- -n",
    "migrate:create": "yarn typeorm migration:create -- -n",
    "migrate:revert": "yarn typeorm migration:revert",
    "seed:config": "ts-node-dev --project ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node-dev --project ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "copy": "copyfiles -u 1 \"src/**/*.nunjucks\" dist/"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "faker": "^5.5.3",
    "graphql": "^15.6.1",
    "handlebar": "^1.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "ldapjs": "^2.3.2",
    "mathjs": "^10.1.1",
    "mime-types": "^2.1.34",
    "module-alias": "^2.2.2",
    "multer": "^1.4.3",
    "nanoid": "^3.1.30",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "typeorm-extension": "^0.3.0",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.1",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.6.4"
  }
}
